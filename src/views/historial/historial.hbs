<div class="row">

    <div class="col-md-4" style="max-height: 550px; overflow-y: auto;">

        {{#each usuarios}}
        <div class="card border-secondary mb-1">
            <div class="card-body user" style="cursor: pointer;" data-id="{{_id}}" data-email="{{email}}">
                <h6 class="card-title">{{email}}</h6>
                <p class="card-text" style="margin-top: -10px;">{{nombre}} - {{tipo}}</p>
            </div>
        </div>
        {{/each}}

    </div>

    <div class="col-md-8">

        <div class="card border-secondary ">
            <div class="card-header" id="actividad">
                <Strong>ACTIVIDAD</Strong>
            </div>
            <div class="card-body " id="historial" data-id="" style="max-height: 500px; overflow-y: auto;">

            </div>
        </div>

    </div>

</div>